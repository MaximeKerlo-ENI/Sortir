<!DOCTYPE html>

<title>Inscriptions index</title>

{% block body %}
    <h1>Inscriptions index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>DateInscription</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for inscription in inscriptions %}
            <tr>
                <td>{{ inscription.dateInscription ? inscription.dateInscription|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_inscriptions_show', {'id': inscription.id}) }}">show</a>
                    <a href="{{ path('app_inscriptions_edit', {'id': inscription.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_inscriptions_new') }}">Create new</a>
{% endblock %}
